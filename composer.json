{
    "name": "emiliomunoz/instagram-feed",
    "description": "This BoltCMS extension allows you to get raw feed data from Instagram's public API using the Access Token.",
    "type": "bolt-extension",
    "keywords": [
        "instagram",
        "social",
        "feed"
    ],
    "require": {
        "php": ">=7.0",
        "bolt/bolt": "^3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^4.7"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Emilio Mu√±oz Monge",
            "email": "emiliomunozmonge@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "autoload": {
        "psr-4": {
            "Bolt\\Extension\\emiliomunoz\\InstagramFeed\\": "src",
            "Vinkla\\Instagram\\": "lib"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Bolt\\Extension\\emiliomunoz\\InstagramFeed\\Tests\\": "tests",
            "Bolt\\Tests\\": "vendor/bolt/bolt/tests/phpunit/unit/"
        }
    },
    "extra": {
        "bolt-assets": "web",
        "bolt-class": "Bolt\\Extension\\emiliomunoz\\InstagramFeed\\InstagramFeedExtension"
    }
}
